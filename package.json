{
  "name": "contexer-sdk",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "ci:publish": "pnpm publish -r",
    "ci:version": "changeset version && pnpm install --frozen-lockfile false",
    "clean": "rm -rf ./{{packages,examples}/*,docs,dashboard}/{dist,umd,.next,.turbo,coverage}",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "release": "pnpm run prerelease && changeset publish",
    "prerelease": "pnpm clean && pnpm install && pnpm build",
    "---------packages---------": "",
    "base": "pnpm --filter \"base\"",
    "logger": "pnpm --filter \"logger\"",
    "---------examples---------": "",
    "react": "pnpm --filter \"@example/react\"",
    "vanilla": "pnpm --filter \"@example/vanilla\""
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.2",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.1"
}
