{
  "name": "@contexter-test/base",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "types": "./lib/main.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "publishConfig": {
    "types": "./dist/main.d.ts"
  },
  "scripts": {
    "dev": "run-p \"dev:*\"",
    "dev:vite": "vite build --watch --emptyOutDir false",
    "build:vite": "vite build",
    "build:tsc": "tsc",
    "build": "run-s \"build:vite\" \"build:tsc\""
  },
  "dependencies": {
    "@contexter-test/logger": "workspace:*",
    "eventemitter3": "^5.0.0",
    "iframe-resizer": "^4.3.2"
  },
  "devDependencies": {
    "eslint": "8.29.0",
    "eslint-config-custom": "workspace:*",
    "npm-run-all": "^4.1.5",
    "tsconfig": "workspace:*",
    "typescript": "4.9.4",
    "vite": "^4.0.1"
  }
}
